{
	"background": {
		"persistent": false,
		"scripts": ["init.js", "background.js"]
	},
	"manifest_version": 2,
	"name": "Oxford Dictionaries (Unofficial)",
	"omnibox": { "keyword" : "o" },
	"options_page": "options.html",
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"permissions": [
		"*://*.oxfordlearnersdictionaries.com/",
		"*://*.oxforddictionaries.com/",
		"storage"
	],
	"version": "0.4"
}